{
  "name": "tests",
  "version": "1.0.0",
  "description": "This package contain all tests",
  "type": "module",
  "author": "Patouillard Franck<patouillardfranck.development@gmail.com>",
  "license": "MIT",
  "scripts": {
    "server:run": "nodemon src/app.js",
    "tests:func": "jest --testPathPattern=./Func",
    "tests:unit": "jest  --testPathPattern=./unit",
    "tests:e2e": "jest  --testPathPattern=./E2E",
    "tests:integration": "jest --testPathPattern=./Integrations",
    "tests:all": "jest *.test.js",
    "tests:func --watch": "jest --watch --testPathPattern=./Func",
    "tests:unit --watch": "jest --watch  --testPathPattern=./unit",
    "tests:e2e --watch": "jest --watch  --testPathPattern=./E2E",
    "tests:integration --watch": "jest --watch --testPathPattern=./Integrations",
    "tests:all --watch": "jest --watchAll"
  },
  "dependencies": {
    "@babel/preset-env": "^7.24.8",
    "babel": "^6.23.0",
    "express": "^4.19.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4"
  }
}
